algoritmo "salario"
var
   ganhoPorHora, horasTrabalhadas, salarioMensal: real
inicio
   escreval("Digite quanto você ganha por hora:200 ")
   leia(ganhoPorHora)

   escreval("Digite o número de horas trabalhadas no mês:8 ")
   leia(horasTrabalhadas)

   salarioMensal := ganhoPorHora * horasTrabalhadas

   escreval("Seu salário no mês é: ", salarioMensal)

fimalgoritmo